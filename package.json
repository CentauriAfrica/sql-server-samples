{
  "name": "sql-server-samples",
  "version": "1.0.2",
  "description": "Official Microsoft GitHub Repository containing code samples for SQL Server",
  "main": "index.js",
  "scripts": {
    "version:patch": "node update-versions.js patch",
    "version:minor": "node update-versions.js minor",
    "version:major": "node update-versions.js major",
    "version:build": "node build-integration.js",
    "version:check": "node update-versions.js patch --dry-run --verbose",
    "version:validate": "node validate-versions.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CentauriAfrica/sql-server-samples.git"
  },
  "keywords": [
    "sql-server",
    "microsoft",
    "database",
    "samples",
    "version-management"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "shelljs": "^0.8.5"
  }
}
